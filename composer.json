{
    "name": "sanderj16/sweet-scent",
    "description": "A PHPCS Coding standard extension",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "squizlabs/php_codesniffer": "^3.5.3"
    },
    "autoload": {
        "psr-4": {
            "SweetScent\\" : "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0@dev"
    },
    "autoload-dev": {
        "psr-4": {
            "SweetScent\\Tests\\" : "tests/"
        }
    }
}
